<Window x:Class="Bibliotech.View.Users.UsersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bibliotech.View.Users"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Icon="/Bibliotech;component/Resources/ico_bibliotech.ico"
        Title="Usuários" Height="720" Width="1280">
    <Grid>

        <DataGrid
            Margin="0,87,0,143">
            
            <DataGrid.Columns>
                <DataGridTextColumn Header="TIPO DE USUÁRIO" Width="150"/>
                <DataGridTextColumn Header="NOME" Width="*"/>
                <DataGridTextColumn Header="TELEFONE" Width="100"/>
                <DataGridTextColumn Header="ENDEREÇO" Width="*"/>
                <DataGridTextColumn Header="DATA DE NASCIMENTO" Width="175"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
